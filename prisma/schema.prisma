generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id       String   @id @default(cuid())
  name     String?
  username String   @unique
  email    String   @unique
  password String
  labels   labels[]
}

model labels {
  id      String         @id @default(cuid())
  name    String
  user    users          @relation(fields: [user_id], references: [id])
  user_id String
  images  label_images[]
}

model label_images {
  id       String @id @default(cuid())
  url      String
  name     String
  labels   labels @relation(fields: [label_id], references: [id])
  label_id String
}
